{
    "env": {
        "browser": true,
        "commonjs": false,
        "es2021": true,
        "jest": true
    },
    "extends": ["google"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "indent": 0,
        "quote-props": 0,
        "max-len": [2, 120, 2],
        "space-before-function-paren": 0,
        "new-cap": "warn",
        "operator-linebreak": "off",
        "no-undef": "error",
        "no-const-assign": "error",
        "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "no-async-promise-executor": "error",
        "object-curly-spacing": "off"
    }
}
